#include <iostream>
#include <string>

class Node
{
		std::string name;
	public:
		Node(){name = "";}
		Node(std::string inName) {name = inName;}
		void printName();	
};

void Node::printName()
{
	std::cout << name << std::endl;
};

class Square
{
		std::string name;
		int x;
		int y;
	public:
		Square(std::string inName) {name = inName;}
		void printName();
};
void Square::printName() 
{
	std::cout << name << std::endl;
};
